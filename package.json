{
  "name": "Scorpio",
  "main": "index.js",
  "version": "1.0.0",
  "repository": {
    "type": "github",
    "url": "https://github.com/TeamCM/scorpio-desktop"
  },
  "description": "Scorpio is an app to chat as Discord app, but updates is made by community",
  "scripts": {
    "start": "electron .",
    "test": "electron .",
    "dist": "electron-builder --win"
  },
  "devDependencies": {
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "electron-builder-squirrel-windows": "^22.9.1"
  },
  "license": "GPL-3.0-only",
  "author": {
    "name": "TeamCM",
    "email": "theteamcm321@gmail.com"
  },
  "publisher": "TeamCM",
  "build": {
    "files": [
      "**/*",
      "build/icon.*"
    ],
    "squirrelWindows": {
      "iconUrl": "http://localhost/favicon.ico",
      "msi": false,
      "remoteReleases": "https://github.com/TeamCM/scorpio-desktop"
    },
    "win": {
      "target": "squirrel",
      "icon": "http://localhost/favicon.ico"
    }
  },
  "dependencies": {
    "electron-log": "^4.3.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^4.3.5"
  }
